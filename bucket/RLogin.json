{
    "homepage": "https://kmiya-culti.github.io/RLogin/",
    "version": "2.26.3",
    "license": "MIT",
    "description": "RLogin",
    "architecture": {
        "64bit": {
            "url": "https://github.com/kmiya-culti/RLogin/files/7063173/rlogin_x64.zip",
            "hash": "cff0f858a856e7d239e72a39d13dfd018e32c3ebfa52b45b263e71b14a0946d2"
        },
        "32bit": {
            "url": "https://github.com/kmiya-culti/RLogin/files/7063171/rlogin_x32.zip",
            "hash": "38eb3ff4f243ebadd981cb4a188b64f6dc530f6c82b76b2d96d16236bb4e8416"
        }
    },
    "bin": "RLogin.exe",
    "shortcuts": [
        [
            "RLogin.exe",
            "RLogin"
        ]
    ],
    "post_install": [
        "",
        "$shortcuts = @(arch_specific 'shortcuts' $manifest $arch)",
        "$shortcutName = $shortcuts[0].item(1)",
        "$scoop_startmenu_folder = shortcut_folder $global",
        "$wsShell = New-Object -ComObject WScript.Shell",
        "$shortcut = $wsShell.CreateShortcut(\"$scoop_startmenu_folder\\$shortcutName.lnk\")",
        "$shortcut.WorkingDirectory = \"$dir\"",
        "$shortcut.Save()",
        ""
    ],
    "checkver": {
        "url": "https://mwlab.net/scoop.cgi",
        "re": "rLogin:(?<version>.*):32bit=(?<x32>[\\d]*):64bit=(?<x64>[\\d]*)",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/kmiya-culti/RLogin/files/$matchX64/rlogin_x64.zip"
            },
            "32bit": {
                "url": "https://github.com/kmiya-culti/RLogin/files/$matchX32/rlogin_x32.zip"
            }
        }
    }
}
